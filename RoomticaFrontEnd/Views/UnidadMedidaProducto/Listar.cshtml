@model IEnumerable<RoomticaFrontEnd.Models.UnidadMedidaProductoModel>

@{
    ViewData["Title"] = "ListarUnidadMedidaProducto";
}

<h1>Listar Unidad Medida Producto</h1>
<div>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Caracterisiticas</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Unidad</td>
                    <td>
                        <a asp-action="Details" asp-route-id="@item.Id">Detail</a> |
                        <a asp-action="Editar" asp-route-id="@item.Id">Editar</a> |
                        <a asp-action="Eliminar" asp-route-id="@item.Id">Eliminar</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
    <a asp-action="Crear">Crear Nueva Caracteristica Habitacion</a>
</div>
