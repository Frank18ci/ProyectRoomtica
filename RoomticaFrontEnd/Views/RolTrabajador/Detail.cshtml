@model RoomticaFrontEnd.Models.RolTrabajadorModel

@{
    ViewData["Title"] = "Detalles de Categoría";
}

<h2>@ViewData["Title"]</h2>

<div>
    <h4>Categoría de Producto</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID
        </dt>
        <dd class="col-sm-10">
            @Model.Id
        </dd>

        <dt class="col-sm-2">
            Rol
        </dt>
        <dd class="col-sm-10">
            @Model.rol
        </dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar</a>
    <a asp-action="Index" class="btn btn-secondary">Volver al listado</a>
</div>