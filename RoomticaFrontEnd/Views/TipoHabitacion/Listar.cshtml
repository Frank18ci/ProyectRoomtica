@model List<RoomticaFrontEnd.Models.TipoHabitacionModel>

@{
    ViewData["Title"] = "Listar";
}

<h1>Listado de Tipos de Habitación</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Tipo</th>
            <th>Descripción</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Tipo</td>
                <td>@item.descripccion</td>

            </tr>
        }
    </tbody>
</table>